@page
@model Hotel_Management_Web.Pages.ManageStaff.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Room</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Room.StaffId)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Room.HotelId)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Room.StaffName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Room.Email)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Room.Password)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Room.Status)
        </dd>
       
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Room.StaffId">Edit</a> |
